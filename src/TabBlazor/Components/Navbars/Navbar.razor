@inherits TablerBaseComponent
@namespace NGageUI

<CascadingValue Name="Navbar" Value=this>
    <HtmlElement Tag="@HtmlTag"
                 class="@ClassNames"
                 @onclick="OnClick"
                 @attributes="@UnmatchedParameters"
                 data-bs-theme="@Background.ToString().ToLower()">

        <div class="container-fluid">

            <button class="navbar-toggler" type="button" @onclick="ToogleExpand">
                <span class="navbar-toggler-icon"></span>
            </button>

           
                @ChildContent
          


        </div>
    </HtmlElement>
</CascadingValue>